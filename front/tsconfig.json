{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "models/*": [
        "./src/models/*"
      ],
      "views/*": [
        "./src/views/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}