---
import { type EkintzaSnippet } from "models/Ekintza";
import { EkintzaZerrenda as EkintzaZerrendaReact } from "./react/EkintzaZerrenda";

export interface Props {
  ekintzak: EkintzaSnippet[];
  title?: string;
  description?: string;
  isMainTitle?: boolean;
}

const { ekintzak, title, description, isMainTitle = false } = Astro.props;
---

<EkintzaZerrendaReact
  ekintzak={ekintzak}
  title={title}
  description={description}
  isMainTitle
  client:idle
/>
