---
import { type EkintzaSnippet } from "models/Ekintza";
import { EkintzaZerrenda as EkintzaZerrendaReact } from "./react/EkintzaZerrenda";

interface Props {
  ekintzak: EkintzaSnippet[];
  title?: string;
  description?: string;
}

const { ekintzak, title, description } = Astro.props;
---

<EkintzaZerrendaReact
  ekintzak={ekintzak}
  title={title}
  description={description}
  client:idle
/>
